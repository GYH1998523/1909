page {
  background-color: white;
}
.addList {
  width: 750rpx;
  height: 232rpx;
  padding: 25rpx;
}
.addList view {
  line-height: 50rpx;
  font-size: 32rpx;
  font-family: _;
  font-weight: 500;
  color: #252525;
}
.addList .user_nameTEl {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #252525;
}
.addList .user_add {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #7a7a7a;
}
.addList .edit_list {
  height: 100rpx;
  border-bottom: 1rpx solid #D3D3D3;
}
.addList .edit_list .list {
  float: left;
}
.addList .edit_list .list .image {
  width: 30rpx;
  height: 30rpx;
  float: left;
}
.addList .edit_list .list .image image {
  width: 100%;
  height: 100%;
  margin: 0 auto;
}
.bg-register {
  width: 706rpx;
  height: 86rpx;
  background: -webkit-gradient(linear, left top, left bottom, from(#32acff), to(#1c7df6));
  background: -webkit-linear-gradient(top, #32acff, #1c7df6);
  background: linear-gradient(180deg, #32acff, #1c7df6);
  border-radius: 43rpx;
  margin: 400rpx 22rpx 0rpx 22rpx;
  font-size: 36rpx;
  color: #f8f8f8;
}
.scroll {
  width: 100%;
  height: 690rpx;
  overflow: hidden;
  overflow-x: hidden;
  overflow-y: scroll;
}

