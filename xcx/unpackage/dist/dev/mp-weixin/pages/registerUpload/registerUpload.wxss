.upLoad {
  width: 636rpx;
  height: 316rpx;
  background: #fafafa;
  border-radius: 20px;
  margin: 57rpx 57rpx 0 57rpx;
  position: relative;
}
.upLoad .cu-bar {
  background: #fafafa;
}
.upLoad .grid {
  background: #fafafa;
}
.upLoad .upLoadIcon {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  margin: 68rpx 268rpx 0rpx 268rpx;
}
.upLoad .upLoadIcon .up1 {
  width: 22rpx;
  height: 100rpx;
  background-color: blue;
  margin-left: 39.2rpx;
  background: #eeeeee;
  border-radius: 11rpx;
}
.upLoad .upLoadIcon .up2 {
  height: 22rpx;
  width: 100rpx;
  background: #eeeeee;
  position: absolute;
  bottom: 39.2rpx;
  border-radius: 11rpx;
}
.upLoad .upLoadText {
  position: absolute;
  left: 194rpx;
  bottom: 66.9rpx;
  width: 270rpx;
  height: 28rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
